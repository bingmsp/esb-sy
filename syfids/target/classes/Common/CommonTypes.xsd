            <xs:element name="InformationRemark" type="InformationRemark" nillable="true" minOccurs="0" maxOccurs="2"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="GateOpenDateTime">
        <xs:annotation>
            <xs:documentation>Date and time of planned gate opening for a
