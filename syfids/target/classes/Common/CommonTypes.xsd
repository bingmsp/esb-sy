            <xs:element name="ScheduledDate" type="ScheduledDate" nillable="true" minOccurs="0"/>
            <xs:element name="AIDXRepeatNumber" type="AidxRepeatNumber" nillable="true" minOccurs="0"/>
            <xs:element name="FlightOriginDate" type="FlightlegOriginDate" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="DisplayFunction">
        <xs:annotation>
            <xs:documentation>The display function of the FIDS page.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="128"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DiversionReasonCode">
        <xs:annotation>
            <xs:documentation>Diversion reason code, free text input for user.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DivertAirportIATACode">
        <xs:annotation>
            <xs:documentation>IATA code of diversionary airport.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="LocationIATACode"/>
    </xs:simpleType>
    <xs:simpleType name="DomesticPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of passengers on board that are
                to/from a domestic sector.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="EconomyClassPassengerCapacity">
        <xs:annotation>
            <xs:documentation>Total capacity of economy class passengers that an
                aircraft can accommodate.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="EconomyClassPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of economy class passengers on board.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="EnglishLanguageText">
        <xs:annotation>
            <xs:documentation>English text (for remarks and look-up codes).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="90"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="EstimatedDateTime">
        <xs:annotation>
            <xs:documentation>Estimated date and time of the arrival or the departure. This field has been left in for backwards compatibility for legacy systems. New EuroControls fields should be used in preference.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="EstimatedInBlockTime">
        <xs:annotation>
            <xs:documentation>The estimated time the aircraft arrives on blocks.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:complexType name="EstimatedInBlockTimes">
        <xs:annotation>
            <xs:documentation>These fields all relate to the estimated time the aircraft arrives on blocks. This is usually a calculated value from a taking into account the EstimatedLandingTime and the Taxi-inTime. The elements here include a 'best' value, EstimatedInBlockTime, as well as values from various sources.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="EstimatedInBlockTime" type="EstimatedInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="EstimatedInBlockTimeTentative" type="EstimatedInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="EstimatedInBlockTimeATCTower" type="EstimatedInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="EstimatedInBlockTimeManual" type="EstimatedInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="EstimatedInBlockTimeRadar" type="EstimatedInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="EstimatedInBlockTimeSitaMsg" type="EstimatedInBlockTime" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="EstimatedOffBlockTime">
        <xs:annotation>
            <xs:documentation>Estimated date and time the departure leaves blocks. This field is as the FlightPlan Estimated Departure Time. In Europe, EstimatedOffBlockTime is currently messaged between aircraft operators and Eurocontrol. The EstimatedOffBlockTime can be modified, by sending a DLA message to Eurocontrol, where the change in the time is greater than 15 minutes.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="EstimatedFlightDuration">
        <xs:annotation>
            <xs:documentation>Estimated duration of a flight for a sector.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:duration"/>
    </xs:simpleType>
    <xs:simpleType name="FIDSPageNumber">
        <xs:annotation>
            <xs:documentation>Page number of the FIDS page.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FirstBagDateTime">
        <xs:annotation>
            <xs:documentation>Date and time of delivery of first bag into baggage
                carousel.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="FirstClassPassengerCapacity">
        <xs:annotation>
            <xs:documentation>Total capacity of first class passengers that an
                aircraft can accommodate.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FirstClassPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of first class passengers on board.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightCancelCode">
        <xs:annotation>
            <xs:documentation>Indicates that a flight has been Cancelled or is Diverted or is Non-operational.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Cancelled"/>
            <xs:enumeration value="Diverted"/>
            <xs:enumeration value="Non-operational"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SeasonalFlightCancelled">
        <xs:annotation>
            <xs:documentation>Indicates that a seasonal flight has been Cancelled.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightClassificationCode">
        <xs:annotation>
            <xs:documentation>The fine-grain code that classifies the exact purpose of the flight. These codes differ are airport defined values.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="FlightData">
        <xs:annotation>
            <xs:documentation>Collection of flight data common to multiple flight
                messages.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="InterestedInterfaces" type="InterestedInterfaces" minOccurs="0"/>
            <xs:element name="Aircraft" type="Aircraft" nillable="false" minOccurs="0"/>
            <xs:element name="Airport" type="Airport" nillable="false" minOccurs="0"/>
            <xs:element name="Flight" type="Flight" nillable="false" minOccurs="0"/>
            <xs:element name="Load" type="Load" nillable="false" minOccurs="0"/>
            <xs:element name="OperationalTimes" type="OperationalTimes" nillable="false" minOccurs="0"/>
            <xs:element name="PortsOfCall" type="PortsOfCall" nillable="false" minOccurs="0"/>
            <xs:element name="Public" type="Public" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Flight">
        <xs:annotation>
            <xs:documentation>Collection of elements specific to the flight.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element name="AccountCode" type="AccountCode" nillable="true" minOccurs="0"/>
            <xs:element name="CodeShareStatus" type="CodeShareStatus" nillable="true" minOccurs="0"/>
            <xs:element name="DivertAirportIATACode" type="DivertAirportIATACode" nillable="true" minOccurs="0"/>
            <xs:element name="DiversionReasonCode" type="DiversionReasonCode" nillable="true" minOccurs="0"/>
            <xs:element name="DeIcePlatform" type="DeIcePlatform" nillable="true" minOccurs="0"/>
            <xs:element name="DeIceProblem" type="DeIceProblem" nillable="true" minOccurs="0"/>
            <xs:element name="FIDSDisplayIndicators" type="FIDSDisplayIndicators" nillable="true" minOccurs="0"/>
            <xs:element name="FlightCancelCode" type="FlightCancelCode" nillable="true" minOccurs="0"/>
            <xs:element name="FlightClassificationCode" type="FlightClassificationCode" nillable="true" minOccurs="0"/>
            <xs:element name="FlightNatureCode" type="FlightNatureCode" nillable="true" minOccurs="0"/>
            <xs:element name="FlightSectorCode" type="FlightSectorCode" minOccurs="0"/>
            <xs:element name="FlightServiceTypeIATACode" type="FlightServiceTypeIATACode" nillable="true" minOccurs="0"/>
            <xs:element name="FlightStatusCode" type="FlightStatusCode" nillable="true" minOccurs="0"/>
            <xs:element name="GroundServiceType" type="GroundServiceType" nillable="true" minOccurs="0"/>
            <xs:element name="HandlingAgent" type="HandlingAgent" nillable="true" minOccurs="0" maxOccurs="6"/>
            <xs:element name="ICAOFlightIdentity" type="FlightIdentity" nillable="true" minOccurs="0"/>
            <xs:element name="IrregularityDetails" type="IrregularityDetails" nillable="true" minOccurs="0" maxOccurs="1"/>
            <xs:element name="IsGeneralAviationFlight" type="IsGeneralAviationFlight" nillable="true" minOccurs="0"/>
            <xs:element name="IsReturnFlight" type="IsReturnFlight" nillable="true" minOccurs="0"/>
            <xs:element name="IsTransitFlight" type="IsTransitFlight" nillable="true" minOccurs="0"/>
            <xs:element name="LinkedFlight" type="LinkedFlight" nillable="true" minOccurs="0"/>
            <xs:element name="MasterFlight" type="MasterFlight" nillable="true" minOccurs="0"/>
            <xs:element name="MarketingPartnership" type="MarketingPartnership" nillable="true" minOccurs="0"/>
            <xs:element name="MissedApproachCount" type="MissedApproachCount" nillable="true" minOccurs="0"/>
            <xs:element name="Notes" type="Notes" nillable="true" minOccurs="0"/>
            <xs:element name="SchengenFlight" type="SchengenFlight" nillable="true" minOccurs="0"/>
            <xs:element name="SchengenIndicator" type="SchengenIndicator" nillable="true" minOccurs="0"/>
            <xs:element name="SecurityCode" type="SecurityCode" nillable="true" minOccurs="0"/>
            <xs:element name="SecurityFlagIndicator" type="SecurityFlagIndicator" nillable="true" minOccurs="0"/>
            <xs:element name="SecurityRestrictedFlight" type="SecurityRestrictedFlight" nillable="true" minOccurs="0"/>
            <xs:element name="ShareFlight" type="ShareFlight" nillable="true" minOccurs="0" maxOccurs="6"/>
            <xs:element name="SpecialSecurityCheck" type="SpecialSecurityCheck" nillable="true" minOccurs="0"/>
            <xs:element name="SubCarrierType" type="SubCarrierType" nillable="true" minOccurs="0"/>
            <xs:element name="TouchAndGo" type="TouchAndGo" nillable="true" minOccurs="0"/>
            <xs:element name="TransferFlightIdentity" type="TransferFlightIdentity" nillable="true" minOccurs="0" maxOccurs="35"/>
            <xs:element name="SpareFlightFields" type="SpareFlightFields" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="FlightDirection">
        <xs:annotation>
            <xs:documentation>Direction of flight (Arrival, Departure).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Arrival"/>
            <xs:enumeration value="Departure"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightElapsedJetTime">
        <xs:annotation>
            <xs:documentation>Jet powered journey time from an airport
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:duration"/>
    </xs:simpleType>
    <xs:simpleType name="FlightElapsedPropellerTime">
        <xs:annotation>
            <xs:documentation>Propeller powered journey time from an airport.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:duration"/>
    </xs:simpleType>
    <xs:complexType name="FlightIdentification">
        <xs:annotation>
            <xs:documentation>Unique identification of a flight is determined by
                flight identity, flight direction, flight repeat count and scheduled
                date. For scheduled flights, the identification can be supplemented
                by an IATA flight identifier and/or an ICAO flight identifier. The
                flight repeat count is used to determine flights that operate more
                than one time on a single day.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FlightID" type="FlightID" nillable="true" minOccurs="0"/>
            <xs:element name="FlightIdentity" type="FlightIdentity" nillable="false"/>
            <xs:element name="FlightDirection" type="FlightDirection" nillable="false" minOccurs="0"/>
            <xs:element name="FlightRepeatCount" type="FlightRepeatCount" nillable="true" minOccurs="0"/>
            <xs:element name="ScheduledDate" type="ScheduledDate" nillable="false"/>
            <xs:element name="IATAFlightIdentifier" type="IATAFlightIdentifier" nillable="false" minOccurs="0"/>
            <xs:element name="ICAOFlightIdentifier" type="ICAOFlightIdentifier" nillable="false" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FlightFilterData">
        <xs:annotation>
            <xs:documentation>The old and new set of data the IMB requires to
                identify flight filter changes.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CarrierIATACode" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="CarrierIATACode">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="CarrierICAOCode" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="CarrierICAOCode">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="CodeShareStatus" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="CodeShareStatus">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="FlightNatureCode" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="FlightNatureCode">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="FlightSectorCode" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="FlightSectorCode">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="FlightServiceTypeIATACode" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="FlightServiceTypeIATACode">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="FlightType" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="FlightType">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="TerminalCode" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="TerminalCode">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="AirportIATACode" nillable="true" minOccurs="0">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="AirportIATACode">
                            <xs:attribute name="previousValue" type="PreviousValue" use="optional"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="FlightFirstDateTime">
        <xs:annotation>
            <xs:documentation>Start of validity date for this flight's operations
                (includes the flight's scheduled time).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="FlightFrequency">
        <xs:annotation>
            <xs:documentation>Interval of operation in number of weeks (e.g.1 = weekly, 2 = fortnightly, 3 = every three weeks, etc.)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightID">
        <xs:annotation>
            <xs:documentation>FlightID is used by SafeGate in GOS-GXI Interface. This is mapped to APPN_ROW_ID in AODB. This is not mandatory in other projects.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightIdentity">
        <xs:annotation>
            <xs:documentation>Identity of the flight. This is typically comprised
                of flight carrier, flight number and an optional suffix. For general
                aviation flights the format is free text.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightLastDateTime">
        <xs:annotation>
            <xs:documentation>End of validity date for this flight's operations
                (includes the flight's scheduled time).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="FlightlegOriginDate">
        <xs:annotation>
            <xs:documentation>This is the date of the departure part of the flightleg or in a multi-leg flight this is the first leg (origin) departure date.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="FlightlegDepartureDate">
        <xs:annotation>
            <xs:documentation>FlightLeg Departure Date of linked departure flight. i.e. For an arrival flight this is the departure date from the immediately preceeding airport 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="FlightNatureCode">
        <xs:annotation>
            <xs:documentation>The code defining the nature of the flight. For example
    â€˜CARâ€™ = Cargo flight
    â€˜CGOâ€™ = Cargo flight
    â€˜CHTâ€™ = Charter flight
    â€˜CRWâ€™ = Crew only flight
    â€˜FERâ€™ = Ferry Flight
    â€˜FRYâ€™ = Ferry flight
    â€˜GAâ€™ = General Aviation flight
    â€˜GASâ€™ = Fuel stop
    â€˜GENâ€™ = General flight
    â€˜JNTâ€™ = Joint flight
    â€˜MAPâ€™ = Missed App.
    â€˜MILâ€™ = Military flight
    â€˜MLTâ€™ = Military flight
    â€˜NFYâ€™ = Not Approved
    â€˜NRVâ€™ = Non-Revenue
    â€˜PAXâ€™ = Passenger flight
    â€˜RFGâ€™ = Refugee flight
    â€˜SPCâ€™ = Special flight
    â€˜STPâ€™ = Technical Stop
    â€˜TNGâ€™ = Training
    â€˜TSTâ€™ = Technical Test 
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightNatureDescription">
        <xs:annotation>
            <xs:documentation>Description or name of flight nature.For example, passenger, cargo, training.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightNumber">
        <xs:annotation>
            <xs:documentation>The number that is used to identify a flight -
                forms part of the flight identity. The number is zero padded to
                three digits (e.g. '73' will become '073').</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
            <xs:minLength value="1"/>
            <xs:pattern value="[0-9]{1,4}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightOperatesOvernight">
        <xs:annotation>
            <xs:documentation>Indicates whether the flight's corresponding flight
                operates on a different day.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightRepeatCount">
        <xs:annotation>
            <xs:documentation>Flight repeat count, will normally be 0 but can be
                1-99 for repeated arrival or departures.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="99"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AidxRepeatNumber">
        <xs:annotation>
            <xs:documentation>Flight repeat count, will normally be 0 but can be
                1-99 for repeated arrival or departures.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="99"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightSeason">
        <xs:annotation>
            <xs:documentation>Season that the flight operates in. This is typically in the format ANN where A is S for Summer or W for Winter. The NN is the last 2 digits of the year, for example S12 is Summer 2012. Note: These values may change airport to airport.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightSectorCode">
        <xs:annotation>
            <xs:documentation>The sector within which the flight operates (International, Domestic, Trans border).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Domestic"/>
            <xs:enumeration value="International"/>
            <xs:enumeration value="Transborder"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightSectorDescription">
        <xs:annotation>
            <xs:documentation>Description or name of flight sector.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightServiceTypeDescription">
        <xs:annotation>
            <xs:documentation>Description of the flight service type.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightServiceTypeIATACode">
        <xs:annotation>
            <xs:documentation>IATA assigned code for type of flight service.
            â€˜Aâ€™ = additional cargo/mail
            â€˜Bâ€™ = additional passenger shuttle
            â€˜Câ€™ = passenger charter
            â€˜Dâ€™ =General Aviation
            â€˜Eâ€™ = special(FAA) - government charter
            â€˜Fâ€™ = cargo scheduled
            â€˜Gâ€™ = additional passenger scheduled
            â€˜Hâ€™ = cargo charter
            â€˜Iâ€™ = Diplomatic/VIP
            â€˜Jâ€™ = passenger scheduled
            â€˜Kâ€™ = training
            â€˜Lâ€™ = search and rescue
            â€˜Mâ€™ = mail scheduled
            â€˜Nâ€™ = creation of E9Z flights
            â€˜Oâ€™ = charter flight(transit)
            â€˜Pâ€™ = positioning(non revenue)
            â€˜Qâ€™ = passenger/cargo combi scheduled
            â€˜Râ€™ = Return to base (tech. problem)
            â€˜Sâ€™ = diversions
            â€˜Tâ€™ = Technical Test
            â€˜Uâ€™ = air ambulance
            â€˜Vâ€™ = air taxi with PAX
            â€˜Wâ€™ = Military
            â€˜Xâ€™ = technical stop
            â€˜Yâ€™ = flights with flight number not for fids
            â€˜Zâ€™ = slot blocked non fids
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightShareStatus">
        <xs:annotation>
            <xs:documentation>Selected flight share status (master flights or all
                flights) - used to filter messages.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="MasterFlightsOnly"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightStatusCode">
        <xs:annotation>
            <xs:documentation>The status of the flight during its progression
                (AB-Airborne, BD-Boarding, CX-Cancelled, DV-Diverted, ES-Estimated,
                EX-Expected, FB-First Bag, FS-Final Approach, FX-Flight Fixed,
                GA-Gate Attended, GC-Gate Close, GO-Gate Open, LB-Last Bag, LC-Last
                Call, LD-Landed, NI-Next Information, OB-On/Off Blocks, OT-On Time,
                OV-Overshoot, RS-Return To Stand, SH-Scheduled, ZN-Zoning).
                </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightSuffix">
        <xs:annotation>
            <xs:documentation>Optional single character that can be appended to
                some flight identities. Some suffices may have specific meanings,
                for example C=charter flight, P=positioning flight, T=training
                flight, Y=Delayed from yesterday. However, these meanings are not
                standard across all systems so the IATA service type element should
                be used in preference.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="1"/>
            <xs:enumeration value="A"/>
            <xs:enumeration value="B"/>
            <xs:enumeration value="C"/>
            <xs:enumeration value="D"/>
            <xs:enumeration value="E"/>
            <xs:enumeration value="F"/>
            <xs:enumeration value="G"/>
            <xs:enumeration value="H"/>
            <xs:enumeration value="I"/>
            <xs:enumeration value="J"/>
            <xs:enumeration value="K"/>
            <xs:enumeration value="L"/>
            <xs:enumeration value="M"/>
            <xs:enumeration value="N"/>
            <xs:enumeration value="O"/>
            <xs:enumeration value="P"/>
            <xs:enumeration value="Q"/>
            <xs:enumeration value="R"/>
            <xs:enumeration value="S"/>
            <xs:enumeration value="T"/>
            <xs:enumeration value="U"/>
            <xs:enumeration value="V"/>
            <xs:enumeration value="W"/>
            <xs:enumeration value="X"/>
            <xs:enumeration value="Y"/>
            <xs:enumeration value="Z"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightTerminalCode">
        <xs:annotation>
            <xs:documentation>Flight Terminal code associated with the flight's
                airline
