            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StandIsSecure">
        <xs:annotation>
            <xs:documentation>Identifies whether the stand has additional
                security associated with it.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StandPosition">
        <xs:annotation>
            <xs:documentation>Stand identity.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="StandPositionSources">
        <xs:annotation>
            <xs:documentation>Stand identity from various sources. Normally the value StandPosition is used as this is the 'best' value. These alternative sources are used to see the last value set by each source. The current supported sources are 'Planner' - i.e. automatically set by a plnning application or 'Manual' - i.e. manually set by an operator. These elements are only used for distributing data from the data repository (normally the AODB). These fields should not ben used for changing values stored in the data repository.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="StandPositionPlanner" type="StandPosition" nillable="true" minOccurs="0"/>
            <xs:element name="StandPositionManual" type="StandPosition" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="StandPositionOperationType">
        <xs:annotation>
            <xs:documentation>Describes a stand operation including identifying the aircraft, flight, scheduled date and blocks times.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="Identification" type="StandPositionOperationIdentification" nillable="false"/>
            <xs:element name="AircraftRegistration" type="AircraftRegistration" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftSubtypeIATACode" type="AircraftSubtypeIATACode" nillable="true" minOccurs="0"/>
            <xs:element name="FlightIdentity" type="FlightIdentity" nillable="true" minOccurs="0"/>
            <xs:element name="FlightRepeatCount" type="FlightRepeatCount" nillable="true" minOccurs="0"/>
            <xs:element name="ScheduledDate" type="ScheduledDate" nillable="true" minOccurs="0"/>
            <xs:element name="OffBlocksDateTime" type="OffBlocksDateTime" nillable="true" minOccurs="0"/>
            <xs:element name="OnBlocksDateTime" type="OnBlocksDateTime" nillable="true" minOccurs="0"/>
            <xs:element name="AIDXRepeatNumber" type="AidxRepeatNumber" nillable="true" minOccurs="0"/>
            <xs:element name="FlightOriginDate" type="FlightlegOriginDate" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="StandPositionOperationIdentification">
        <xs:annotation>
            <xs:documentation>Identification of a stand operation, this contains the StandPosition and the terminal</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="StandPosition" type="StandPosition" nillable="false"/>
            <xs:element name="TerminalCode" type="TerminalCode" nillable="false" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="StandStatus">
        <xs:annotation>
            <xs:documentation>Operational status of the stand (Unserviceable, Serviceable).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Unservicable"/>
            <xs:enumeration value="Servicable"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StandStatusEndDateTime">
        <xs:annotation>
            <xs:documentation>The stand's status end period.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="StandStatusReason">
        <xs:annotation>
            <xs:documentation>Free text providing additional information
                regarding the status of the resource.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="AbstractTypeStatusReason"/>
    </xs:simpleType>
    <xs:simpleType name="StandStatusStartDateTime">
        <xs:annotation>
            <xs:documentation>The stand's status start period.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="SubscriptionMode">
        <xs:annotation>
            <xs:documentation>The type of subscription required (SnapshotOnly,
                SnapshotThenUpdates, UpdatesOnly).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="SnapshotOnly"/>
            <xs:enumeration value="SnapshotThenUpdates"/>
            <xs:enumeration value="UpdatesOnly"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SupplementaryInformationText">
        <xs:annotation>
            <xs:documentation>Additional information in free format.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TargetOffBlockTime">
        <xs:annotation>
            <xs:documentation>The time that and Aircraft Operator or Ground Handler estimates than an aircraft will be ready, all doors closed, boarding bridge removed, push back vehicle available and ready to start up</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:complexType name="TargetOffBlockTimes">
        <xs:annotation>
            <xs:documentation>The time that and Aircraft Operator or Ground Handler estimates than an aircraft will be ready, all doors closed, boarding bridge removed, push back vehicle available and ready to start up. </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="TargetOffBlockTimeSitaMsg" type="TargetOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="TargetOffBlockTimeManual" type="TargetOffBlockTime" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="TaskAutoAssign">
        <xs:annotation>
            <xs:documentation>Boolean indicating if a task was automatically assigned</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskCategory">
        <xs:annotation>
            <xs:documentation>The category of task â€“ Arrival, Departure, Parking</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Arrival"/>
            <xs:enumeration value="Departure"/>
            <xs:enumeration value="Parking"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskDescription">
        <xs:annotation>
            <xs:documentation>Task description.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskEnabled">
        <xs:annotation>
            <xs:documentation>Indicator for whether a task is enabled: Enabled, Auto Disabled, Manually Disabled</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Enabled"/>
            <xs:enumeration value="Auto Disabled"/>
            <xs:enumeration value="Manual Disabled"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskID">
        <xs:annotation>
            <xs:documentation>Task identity.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="18"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskLocation">
        <xs:annotation>
            <xs:documentation>Task location.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskProgress">
        <xs:annotation>
            <xs:documentation>Task progress 0 - TotalTaskProgress Value.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskRepeatCount">
        <xs:annotation>
            <xs:documentation>Task repeat count</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskTotalProgress">
        <xs:annotation>
            <xs:documentation>Task total progress (normally a percentage but can
                be a passenger count)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaskType">
        <xs:annotation>
            <xs:documentation>Name of a task that can be performed on an aircraft
            </xs:documentation>
        </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:maxLength value="30"/>
      </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ActualInRangeTime">
        <xs:annotation>
            <xs:documentation>Estimated / actual date and time when the aircraft is approximately ten miles from the airport.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="TerminalCode">
        <xs:annotation>
            <xs:documentation>Terminal identity.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DisplayOrder">
        <xs:annotation>
            <xs:documentation>Display Order</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TerminalDescription">
        <xs:annotation>
            <xs:documentation>Description of the terminal.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TextCategoryCode">
        <xs:annotation>
            <xs:documentation>Language category type. This is used to determine
                what text code requires looking up.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TextCode">
        <xs:annotation>
            <xs:documentation>Language code to look-up (e.g. the code to look-up
                to find a remark expansion or a language translation).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalBaggageCount">
        <xs:annotation>
            <xs:documentation>Total number of bags on board.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalBaggageWeight">
        <xs:annotation>
            <xs:documentation>Total baggage weight on board.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalCrewCount">
        <xs:annotation>
            <xs:documentation>Total number of crew on board.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="99"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalFreightWeight">
        <xs:annotation>
            <xs:documentation>Total weight of freight on board.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:totalDigits value="7"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalLoadWeight">
        <xs:annotation>
            <xs:documentation>Total weight of cargo, freight, baggage, mail etc.
                Does not include passengers and crew weight. This is also known as
                deadload.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:totalDigits value="7"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalMailWeight">
        <xs:annotation>
            <xs:documentation>Total weight of mail on board.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="99999"/>
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalAssistPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of assist passengers on board.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="999"/>
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TotalPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of passengers on board (including
                terminal, transit and transfer components).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="999"/>
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransferBaggageCount">
        <xs:annotation>
            <xs:documentation>Total number of bags to transfer.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransferBaggageWeight">
        <xs:annotation>
            <xs:documentation>Total baggage weight to transfer.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransferCargoWeight">
        <xs:annotation>
            <xs:documentation>Total cargo weight to transfer.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
      <xs:simpleType name="TransferClassCode">
      <xs:annotation>
         <xs:documentation>IATA standard class/compartment code.
         </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:maxLength value="1"/>
      </xs:restriction>
   </xs:simpleType>
    <xs:simpleType name="TransferFlightIdentity">
        <xs:annotation>
            <xs:documentation>Flight identity passengers transfer to or from.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransferMailWeight">
        <xs:annotation>
            <xs:documentation>Total mail weight to transfer.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransferPassengerCount">
        <xs:annotation>
            <xs:documentation>Number of passengers on board who are transferring
                to other flights.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="999"/>
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransitBaggageCount">
        <xs:annotation>
            <xs:documentation>Total number of bags in transit.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransitBaggageWeight">
        <xs:annotation>
            <xs:documentation>Total baggage weight in transit.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransitPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of passengers in transit.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="WeekDay">
        <xs:annotation>
            <xs:documentation>Day of the week</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Monday"/>
            <xs:enumeration value="Tuesday"/>
            <xs:enumeration value="Wednesday"/>
            <xs:enumeration value="Thursday"/>
            <xs:enumeration value="Friday"/>
            <xs:enumeration value="Saturday"/>
            <xs:enumeration value="Sunday"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="WheelchairPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of passengers on board requiring
                wheelchair assistance.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="999"/>
            <xs:minInclusive value="0"/>
            <xs:totalDigits value="3" fixed="false"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="WheelsDownDateTime">
        <xs:annotation>
            <xs:documentation>Aircraft's touch-down time.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="WheelsUpDateTime">
        <xs:annotation>
            <xs:documentation>Aircraft's airborne time.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="AODBAlarmText">
        <xs:annotation>
            <xs:documentation>The text describing the current alarm.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1000"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AODBAlarmRaisedAt">
        <xs:annotation>
            <xs:documentation>The date and time that the alarm was raised in the
                system generating this alarm.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:complexType name="AODBAlarm">
        <xs:sequence>
            <xs:element name="AODBAlarmText" type="AODBAlarmText"/>
            <xs:element name="AODBAlarmRaisedAt" type="AODBAlarmRaisedAt"/>
            <xs:element name="AODBAlarmType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Invalid Message Structure"/>
                        <xs:enumeration value="Invalid Message Content"/>
                        <xs:enumeration value="Other"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="AODBAlarmType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="Invalid Message Structure">
                <xs:annotation>
                    <xs:documentation>Indicates the alarm is raised in response to a
                        received invalid message structure.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Invalid Message Content">
                <xs:annotation>
                    <xs:documentation>Indicates the alarm is raised in response to a
                        received message containing invalid data content.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other">
                <xs:annotation>
                    <xs:documentation>Indicates the alarm is raised for alternative
                        purpose.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AODBFlightAlarmText">
        <xs:annotation>
            <xs:documentation>The text describing the flight alarm.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1024"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AODBFlightAlarmNumber">
        <xs:annotation>
            <xs:documentation> The Flight Alarm Number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="1"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="AODBFlightAlarm">
        <xs:annotation>
            <xs:documentation> The Flight Alarm to create or delete</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AODBFlightAlarmNumber" type="AODBFlightAlarmNumber" />
            <xs:element name="AODBFlightAlarmText" type="AODBFlightAlarmText"/>
            <xs:element name="FlightDirection" type="FlightDirection"/>
            <xs:element name="FlightIdentity" type="FlightIdentity"/>
            <xs:element name="ScheduledDate" type="ScheduledDate"/>
            <xs:element name="FlightRepeatCount" type="FlightRepeatCount" nillable="true" minOccurs="0"/>
            <xs:element name="Parameter" nillable="true" minOccurs="0" maxOccurs="10">
               <xs:annotation>
                     <xs:documentation> List of parameters for display in HCI</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:maxLength value="80" />
                  </xs:restriction>
               </xs:simpleType>
            </xs:element>                        
        </xs:sequence>
    </xs:complexType>    
    <xs:simpleType name="AirportIATACode">
        <xs:annotation>
            <xs:documentation>IATA code of the airport associated with the flight.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PortOfCallAirportName">
        <xs:annotation>
            <xs:documentation>Expanded airport name.
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IATACountryCode">
        <xs:annotation>
            <xs:documentation>IATA code of country</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IATACountryName">
        <xs:annotation>
            <xs:documentation>Name of country</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SecurityCode">
        <xs:annotation>
            <xs:documentation>Security Code that refers to a defined set of reference data</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SecurityCodeDescription">
        <xs:annotation>
            <xs:documentation>Description of the security code.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Notes">
        <xs:annotation>
            <xs:documentation>Notes for the seasonal Flight</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TransactionID">
        <xs:annotation>
            <xs:documentation>The Transaction Id associated with a Slot Request
                record.
                Note: a long data type is used to ensure that XML
                generation
                tools cater for very large integer values.
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999999999999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ScheduledTime">
        <xs:annotation>
            <xs:documentation>Scheduled Time for a seasonal Flight in hh:mm:ss format to be used in Slot Request message</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:time"/>
    </xs:simpleType>
    <xs:simpleType name="SCSFlightStatus">
        <xs:annotation>
            <xs:documentation> Flight Status for Slot Coordination System. Initial Load, Delete,  Insert/Update</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Initial Load"/>
            <xs:enumeration value="Delete"/>
            <xs:enumeration value="Insert or Update"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FlightCoordinatedAirport">
        <xs:annotation>
            <xs:documentation>Co-ordinated Airport of Flight.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="LocationIATACode"/>
    </xs:simpleType>
    <xs:simpleType name="ExitDoor">
        <xs:annotation>
            <xs:documentation>Identification of an Exit Door</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ExtendedFlightDataCategory">
        <xs:annotation>
            <xs:documentation>The extended flight data categories. (N.B. The
                "All"
                category is special for service provider processing only, it
                cannot
                be set by client systems.)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="All"/>
            <xs:enumeration value="PassengerAssists"/>
            <xs:enumeration value="PassengerTransfers"/>
            <xs:enumeration value="VIPData"/>
            <xs:enumeration value="VisitTask"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="VIPData">
        <xs:annotation>
            <xs:documentation>The details of one VIP on a flight.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="VIPName" type="VIPName" nillable="false"/>
            <xs:element name="RecordIdentification" type="RecordIdentification"/>
            <xs:element name="VIPLevel" type="VIPLevel"/>
            <xs:element name="Title" type="Title"/>
            <xs:element name="TelephoneNumber" type="TelephoneNumber" minOccurs="0"/>
            <xs:element name="Gender" type="Gender" minOccurs="0"/>
            <xs:element name="Description" type="Description" minOccurs="0"/>
            <xs:element name="NumberOfRetinues" type="NumberOfRetinues" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="VisitTask">
        <xs:annotation>
            <xs:documentation>The details of one task performed on a flight.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
         <xs:element name="TaskID" type="TaskID" nillable="false"/>
         <xs:element name="SourceSystem" type="SourceSystem" nillable="false"/>
         <xs:element name="TaskType" type="TaskType" nillable="false"/>
         <xs:element name="TaskRepeatCount" type="TaskRepeatCount" nillable="false"/>
         <xs:element name="TaskCategory" type="TaskCategory" nillable="false" minOccurs="0"/>
         <xs:element name="TaskAutoAssign" type="TaskAutoAssign" nillable="true" minOccurs="0"/>
         <xs:element name="TaskDescription" type="TaskDescription" nillable="true" minOccurs="0"/>
         <xs:element name="HandlingAgentCode" type="HandlingAgentCode" nillable="true" minOccurs="0"/>
         <xs:element name="TaskLocation" type="TaskLocation" nillable="true" minOccurs="0"/>
         <xs:element name="PlannedTaskDuration" type="PlannedTaskDuration" nillable="true" minOccurs="0"/>
         <xs:element name="MinimumTaskDuration" type="MinimumTaskDuration" nillable="true" minOccurs="0"/>
         <xs:element name="PlannedTaskStartTime" type="PlannedTaskStartTime" nillable="true" minOccurs="0"/>
         <xs:element name="PlannedTaskEndTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
         <xs:element name="CriticalTaskStartTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
         <xs:element name="CriticalTaskEndTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
         <xs:element name="EstimatedTaskStartTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
         <xs:element name="EstimatedTaskEndTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
         <xs:element name="ActualTaskStartTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
         <xs:element name="ActualTaskEndTime" type="xs:dateTime" nillable="true" minOccurs="0"/>
         <xs:element name="NATOnPlannedStart" type="NATOnPlannedStart" nillable="true" minOccurs="0"/>
         <xs:element name="NATOnPlannedEnd" type="NATOnPlannedEnd" nillable="true" minOccurs="0"/>
         <xs:element name="NATOnCriticalStart" type="NATOnCriticalStart" nillable="true" minOccurs="0"/>
         <xs:element name="NATOnCriticalEnd" type="NATOnCriticalEnd" nillable="true" minOccurs="0"/>
         <xs:element name="TaskProgress" type="TaskProgress" nillable="true" minOccurs="0"/>
         <xs:element name="TaskTotalProgress" type="TaskTotalProgress" nillable="false" minOccurs="0"/>
         <xs:element name="TaskEnabled" type="TaskEnabled" nillable="false" minOccurs="0"/>
         <xs:element name="Comment" type="Comment" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="VehicleUsage">
        <xs:annotation>
            <xs:documentation>The details of one vehicle used to service a
                flight.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="RecordIdentification" type="RecordIdentification"/>
            <xs:element name="VehicleType" type="CheckinSlotID" nillable="false"/>
            <xs:element name="VehicleID" type="CheckinDeskID" nillable="false"/>
            <xs:element name="StartTime" type="xs:dateTime"/>
            <xs:element name="EndTime" type="xs:dateTime" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ExtendedFlightData">
        <xs:annotation>
            <xs:documentation>Collection of extended flight data common to
                extended flight data messages.</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="PassengerAssists" type="PassengerAssists" nillable="false" minOccurs="0"/>
            <xs:element name="PassengerTransfers" type="PassengerTransfers" nillable="false" minOccurs="0"/>
            <xs:element name="VIPData" type="VIPData" nillable="false" minOccurs="0"/>
            <xs:element name="VisitTask" type="VisitTask" nillable="false" minOccurs="0"/>
            <xs:element name="VehicleUsage" type="VehicleUsage" nillable="false" minOccurs="0"/>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="ExtendedSeasonalFlightDataCategory">
        <xs:annotation>
            <xs:documentation>The extended seasonal flight data categories. (N.B.
                The "All"
                category is special for service provider processing only,
                it cannot
                be set by client systems.)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="All"/>
            <xs:enumeration value="SeasonalPortsOfCallArrival"/>
            <xs:enumeration value="SeasonalPortsOfCallDeparture"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ExtendedSeasonalFlightData">
        <xs:annotation>
            <xs:documentation>Collection of extended flight data common to
                extended seasonal flight data messages.</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="SeasonalPortsOfCallArrival" type="SeasonalPortsOfCall" nillable="false" minOccurs="0"/>
            <xs:element name="SeasonalPortsOfCallDeparture" type="SeasonalPortsOfCall" nillable="false" minOccurs="0"/>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="VIPName">
        <xs:annotation>
            <xs:documentation>Name of the VIP boarding the Flight
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="VIPLevel">
        <xs:annotation>
            <xs:documentation>Importance of the VIP on the Flight
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="VVIP"/>
            <xs:enumeration value="VIP"/>
            <xs:enumeration value="CIP"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Title">
        <xs:annotation>
            <xs:documentation>Title of the VIP on the Flight</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TelephoneNumber">
        <xs:annotation>
            <xs:documentation>Title of the VIP on the Flight</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Gender">
        <xs:annotation>
            <xs:documentation>Gender of a person, e.g. for VIP on a flight</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Male"/>
            <xs:enumeration value="Female"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Description">
        <xs:annotation>
            <xs:documentation>Description of the VIP / Journey on the Flight
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="1000"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NumberOfRetinues">
        <xs:annotation>
            <xs:documentation>Number of retinues accompanying the VIP on the
                Flight</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftVisitSequence">
        <xs:annotation>
            <xs:documentation>The tow number associated with an ordered tow
                sequence. Note: a long data type is used to ensure that XML
                generation tools cater for very large integer values.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999999999999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="AircraftVisitIdentification">
        <xs:sequence>
            <xs:element name="AircraftVisitSequence" type="AircraftVisitSequence" nillable="false"/>
            <xs:element name="AircraftRegistration" type="AircraftRegistration" nillable="false"/>
            <xs:element name="ScheduledDate" type="ScheduledDate" nillable="false"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RecordIdentification">
        <xs:annotation>
            <xs:documentation>Name of originating system and its sequence number
                to uniquely identify the record</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="OriginatingSystem">
                <xs:annotation>
                    <xs:documentation>Originating system of the record
                    </xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="20"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="OriginatingID">
                <xs:annotation>
                    <xs:documentation>Original system's unique sequence number for the
                        record</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:long">
                        <xs:maxInclusive value="999999999999999999"/>
                        <xs:minInclusive value="0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="MarketingPartnership">
        <xs:annotation>
            <xs:documentation>The name of marketing partnership (e.g. Star) under
                which this
                flight leg is operating</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CarouselManuallySet">
        <xs:annotation>
            <xs:documentation>Flag to indicate if BaggageReclaimCarouselID has been manually set.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GateManuallySet">
        <xs:annotation>
            <xs:documentation>Flag to indicate if gate has been manually set.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RampText">
        <xs:annotation>
            <xs:documentation>Ramp Information free text</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ICAOAirportCode">
        <xs:annotation>
            <xs:documentation>ICAO Airport code</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PublicAirlineLog">
        <xs:annotation>
            <xs:documentation>Public airline log (ICAO code)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MissedApproachCount">
        <xs:annotation>
            <xs:documentation>Number of missed attempts at approach / landing</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer"/>
    </xs:simpleType>
    <xs:simpleType name="LastTransaction">
        <xs:annotation>
            <xs:documentation>LastTransaction indicator for Slot Request. 1 if
                record is the last record
                in an initial load or last record within a
                given flight update
                otherwise 0
        </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="OverMidnightIndicator">
        <xs:annotation>
            <xs:documentation>Over Midnight Indicator - this indicates the number
                of days, normally used for indicating length of time an aircraft is
                on the ground.
        </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftFirstRegistrationDate">
        <xs:annotation>
            <xs:documentation>The date that the aircraft was first registered.
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="AircraftCurrentRegistrationDate">
        <xs:annotation>
            <xs:documentation>Date of last occasion the aircraft was registered, i.e. When did the aircraft registration take place that is currently valid.
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="AircraftAccountCode">
        <xs:annotation>
            <xs:documentation>Determines the account code for an aircraft. Used for billing purposes. Usually this is the airline IATA code but this may change at different airports.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftActiveStartTime">
        <xs:annotation>
            <xs:documentation>This is the timestamp from when the aircraft is valid for use. The end of the active period is provided by the AircraftActiveEndTime.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="AircraftActiveEndTime">
        <xs:annotation>
            <xs:documentation>This is the timestamp after which the aircraft is no longer valid for use. The initial start of the active period is provided by the AircraftActiveStartTime
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="Latitude">
        <xs:annotation>
            <xs:documentation>The latitude of this location (in degrees north of
                the equator, negative values are south of the equator)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="5"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Longitude">
        <xs:annotation>
            <xs:documentation>The longitude of this location (in degrees east of
                Greenwich, negative values are west of Greenwich)
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:totalDigits value="5"/>
            <xs:fractionDigits value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AltLanguage">
        <xs:annotation>
            <xs:documentation>Alternative Language Code(s) for this airport
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AutoTow">
        <xs:annotation>
            <xs:documentation>Automated (or Unconfirmed Tow) flag. This flag is used to notify planning applications that this flight should have a tow automatically created for it.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IATAPriority">
        <xs:annotation>
            <xs:documentation>Priority order of duplicate IATA codes
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="99"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AverageWeight">
        <xs:annotation>
            <xs:documentation>Average weight of a passenger</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CheckInAisleRange">
        <xs:annotation>
            <xs:documentation>The check-in aisle range for the airline for the
                current day</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DisplayOnCheckInSummary">
        <xs:annotation>
            <xs:documentation>Indicates that the airline should be shown on the check-in/airline location summary pages
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BlackListedDate">
        <xs:annotation>
            <xs:documentation>Date at which the airline became blacklisted
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="LocalDisembarkingPassengerCount">
        <xs:annotation>
            <xs:documentation>Number of Local/Disembarking passengers
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AvailableForDisembarkationPassengerCount">
        <xs:annotation>
            <xs:documentation>Number of passengers available for disembarkation
                (e.g. Non-Revenue/Staff)
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AirlineIATANumericCode">
        <xs:annotation>
            <xs:documentation>Numeric code for this airline</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="9999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LocationRegionIndicator">
        <xs:annotation>
            <xs:documentation>Indicator for Location region data</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Added Ext">
                <xs:annotation>
                    <xs:documentation>Location Region data was set by an external data source (to FIMS)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Added">
                <xs:annotation>
                    <xs:documentation> Location Region data was set by an manual (FIMS User) data source </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Removed Ext">
                <xs:annotation>
                    <xs:documentation> Location Region data was removed by an external data source (to FIMS) </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LocationRegion">
        <xs:annotation>
            <xs:documentation>Defines region of location (e.g. AF - Africa, AS -
                Asia, EU - Europe,
                NA - North America, SA - South America, AU -
                Australia)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FreeFormatRemark">
        <xs:annotation>
            <xs:documentation>Free format remark</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FIDSDisplayIndicator">
        <xs:annotation>
            <xs:documentation>Indicator providing information to FIDS displays to roll flight on or off displays (These FIDS maybe Baggage, Public or Staff)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Auto Off"/>
            <xs:enumeration value="Manual Off"/>
            <xs:enumeration value="Roll On"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MarshallerRequired">
        <xs:annotation>
            <xs:documentation>Indicator to specify whether extra marshalling is required for this aircraft: Set on by external source,  manually set on, set off by external source.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Req">
                <xs:annotation>
                    <xs:documentation> Manually set as required in the FIMS</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Req Ext">
                <xs:annotation>
                    <xs:documentation> Marshaller required, set by a system outside of FIMS </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Not Req">
                <xs:annotation>
                    <xs:documentation>Requirement for marshaller set off by an external to FIMS source</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="MaintenanceType">
        <xs:annotation>
            <xs:documentation>The type of maintenance (if any) to be carried out.
                Codes are often used - these will be airline defined
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="NoOfTakeoffLandings">
        <xs:annotation>
            <xs:documentation>Number of Take of and Landings â€“ this maybe related to a touch and go flight i.e. the number of aborted attempts at landing, or it maybe due to a training flight that is landind for a short period before taking off again</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:int">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="99"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="RadioFrequency">
        <xs:annotation>
            <xs:documentation>The radio frequency used by the aircraft flight
                crew</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TargetTowStand">
        <xs:annotation>
            <xs:documentation>For an arrival record (i.e. not a parking/tow
                record) this shows the target tow stand</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PierCode">
        <xs:annotation>
            <xs:documentation>Identification of a pier</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SubCarrierType">
        <xs:annotation>
            <xs:documentation>Type of sub-carrier, express carrier, connection
                carrier etc.
                Used to indicate that the airline requires that data for
                this flight to
                be displayed in a unique way (e.g. Ted, Song etc)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SchengenFlight">
        <xs:annotation>
            <xs:documentation>Boolean indicating whether this is a Schengen flight. A schengen flight is one that travels between different countries that have a schengen agreement relating to borderless flights</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
   <xs:simpleType name="SchengenIndicator">
      <xs:annotation>
         <xs:documentation>Schengen flight Indicator set by</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="Added Ext">
            <xs:annotation>
               <xs:documentation>Schengen indicator was set by an external data source (to FIMS)</xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Added">
            <xs:annotation>
               <xs:documentation>Schengen indicator was set by an manual (FIMS User) data source </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="Removed Ext">
            <xs:annotation>
               <xs:documentation>Schengen indicator was removed by an external data source (to FIMS) </xs:documentation>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>
    <xs:simpleType name="SecurityFlagIndicator">
        <xs:annotation>
            <xs:documentation>Indicator for Security code. Nilled item is initial setting or cleared by a FIMS user </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Added">
                <xs:annotation>
                    <xs:documentation>Security Code has been set manually by a FIMS user</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Added Ext">
                <xs:annotation>
                    <xs:documentation>Security Code has been set externally from FIMS users</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Removed Ext">
                <xs:annotation>
                    <xs:documentation>Security Code has been removed externally from FIMS users</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
   <xs:simpleType name="SecurityRestrictedFlight">
      <xs:annotation>
         <xs:documentation>Indicates if the flight shoudl be restricted from view</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:boolean">
         <xs:pattern value="false|true"/>
      </xs:restriction>
   </xs:simpleType>
    <xs:simpleType name="SpecialSecurityCheck">
        <xs:annotation>
            <xs:documentation>Indicates if the flight leg has to have an additional / special security check</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GroundServiceType">
        <xs:annotation>
            <xs:documentation>The type of ground service to be carried out. (e.g.
                quick clean or full cleaning). These will be airline defined.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="StandManuallySet">
        <xs:annotation>
            <xs:documentation>Flag to indicate if StandPosition has been manually (e.g. HCI) set</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TouchAndGo">
        <xs:annotation>
            <xs:documentation>This is a flag set when a flight is a â€˜Touch and Goâ€™ â€“ this is where the flight approaches but only touches the runway and takes off again due to a problem</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PlaneTailNumber">
        <xs:annotation>
            <xs:documentation>Plane Tail Number - REG Number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FleetNumber">
        <xs:annotation>
            <xs:documentation>Defines the airline fleet number of the aircraft
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="6"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="SpareFlightFields">
        <xs:annotation>
            <xs:documentation>This is a set of spare fields that can be used to represent airport specific data where there is a desire to not remodel the XML schema.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SpareFlightCheckbox1" type="SpareFlightCheckbox" nillable="true" minOccurs="0"/>
            <xs:element name="SpareFlightCheckbox2" type="SpareFlightCheckbox" nillable="true" minOccurs="0"/>
            <xs:element name="SpareFlightDate1" type="SpareFlightDate" nillable="true" minOccurs="0"/>
            <xs:element name="SpareFlightDate2" type="SpareFlightDate" nillable="true" minOccurs="0"/>
            <xs:element name="SpareFlightCharacter1" type="SpareFlightCharacter" nillable="true" minOccurs="0"/>
            <xs:element name="SpareFlightCharacter2" type="SpareFlightCharacter" nillable="true" minOccurs="0"/>
            <xs:element name="SpareFlightNumber1" type="SpareFlightNumber" nillable="true" minOccurs="0"/>
            <xs:element name="SpareFlightNumber2" type="SpareFlightNumber" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="SpareFlightCheckbox">
        <xs:annotation>
            <xs:documentation>Spare flight checkbox field</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SpareAircraftDate">
        <xs:annotation>
            <xs:documentation>Spare aircraft date field</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="SpareFlightDate">
        <xs:annotation>
            <xs:documentation>Spare Flight date field</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:date"/>
    </xs:simpleType>
    <xs:simpleType name="SpareFlightCharacter">
        <xs:annotation>
            <xs:documentation>Spare flight character field.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SpareAircraftCharacter">
        <xs:annotation>
            <xs:documentation>Spare aircraft character field.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SpareFlightNumber">
        <xs:annotation>
            <xs:documentation>Spare flight number field.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999999999999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SpareAircrafttNumber">
        <xs:annotation>
            <xs:documentation>Spare aircraft number field.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999999999999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="IrregularityCode">
        <xs:annotation>
            <xs:documentation>Irregularity code (User defined).
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AirborneDateTime">
        <xs:annotation>
            <xs:documentation>Actual airborne time</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="CheckinRow">
        <xs:annotation>
            <xs:documentation>Details describing the Check-in row</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CheckinRangeIndicator">
        <xs:annotation>
            <xs:documentation>Check-in range indicator. This is indicating whether the checkin information is applicable to a single range of desks or both ranges of desks. </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="First Only">
                <xs:annotation>
                    <xs:documentation>Applicable to First Checkin Range Only </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Second Only">
                <xs:annotation>
                    <xs:documentation>Applicable to Second Checkin Range Only </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Both Ranges">
                <xs:annotation>
                    <xs:documentation>Applicable to Both Checkin Ranges </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TakeOffTime">
        <xs:annotation>
            <xs:documentation>Eurocontrol Calculated Takeoff Time (CTOT)
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="DeIcePlatform">
        <xs:annotation>
            <xs:documentation>Indicates the de-icing platform this aircraft is from</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DeIceProblem">
        <xs:annotation>
            <xs:documentation>Indicates that de-icing this aircraft is a problem. For example the ground handlers may not have a truck big enough for the aircraft</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FinalCallTime">
        <xs:annotation>
            <xs:documentation>Final Call Time at Gate</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="FuelAmount">
        <xs:annotation>
            <xs:documentation>Amount of fuel</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="BoardingTime">
        <xs:annotation>
            <xs:documentation>Gate Boarding time/date</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="LastCallTime">
        <xs:annotation>
            <xs:documentation>Last Call at a Gate</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:element name="WSException" type="WSException"/>
    <xs:complexType name="WSException">
        <xs:annotation>
            <xs:documentation>Exception that can be returned in the event of a failed Web Service call.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="code" type="xs:string" minOccurs="0"/>
            <xs:element name="message" type="xs:string" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ReportErrors">
        <xs:annotation>
            <xs:documentation> When used in Request type services (e.g. AFUS, RDUS, BDUS etc) this element can ensure that any database errors are reported all the way back to the client system. When this feature is used the client must ensure that any returned errors are flagged for manual intervention as there was a problem with the original request that must be fixed for the data to be accepted by the AODB.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:boolean">
            <xs:pattern value="false|true"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="FIDSDisplayIndicators">
        <xs:annotation>
            <xs:documentation>These indicators provides instructions on whether the flight should be displayed (or removed from the display)</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="FIDSBaggageDisplay" type="FIDSDisplayIndicator" nillable="true" minOccurs="0"/>
            <xs:element name="FIDSGateDisplay" type="FIDSDisplayIndicator" nillable="true" minOccurs="0"/>
            <xs:element name="FIDSPublicDisplay" type="FIDSDisplayIndicator" nillable="true" minOccurs="0"/>
            <xs:element name="FIDSStaffDisplay" type="FIDSDisplayIndicator" nillable="true" minOccurs="0"/>
            <xs:element name="FIDSLandsidePublicDisplay" type="FIDSDisplayIndicator" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
	<xs:complexType name="RunwayOperationIdentification">
		<xs:sequence>
			<xs:element name="PhysicalRunway" type="RunwayID" nillable="false">
				<xs:annotation>
					<xs:documentation>Name of the physical runway in use.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
