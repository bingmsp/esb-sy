            <xs:element name="BoardingTime" type="BoardingTime" nillable="true" minOccurs="0"/>
            <xs:element name="LastCallTime" type="LastCallTime" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="GateBoardingStatus">
        <xs:annotation>
            <xs:documentation>Status of boarding process within a departure gate, which provides additional detail to the main FlightStatus (Open, Attended, Boarding, Last Call, Close).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Attended"/>
            <xs:enumeration value="Boarding"/>
            <xs:enumeration value="Closed"/>
            <xs:enumeration value="Final Call"/>
            <xs:enumeration value="Last Call"/>
            <xs:enumeration value="Open"/>
        </xs:restriction>
