            <xs:element name="GateRole" type="GateRole" nillable="false"/>
            <xs:element name="GateActualCloseDateTime" type="GateActualCloseDateTime" nillable="true" minOccurs="0"/>
            <xs:element name="GateActualOpenDateTime" type="GateActualOpenDateTime" nillable="true" minOccurs="0"/>
            <xs:element name="GateArea" type="GateArea" nillable="true" minOccurs="0"/>
            <xs:element name="GatePodium1" type="GatePodium" nillable="true" minOccurs="0"/>
