    <xs:simpleType name="GateCloseDateTime">
        <xs:annotation>
            <xs:documentation>Date and time of planned gate closure for a
                departing flight.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="GateActualCloseDateTime">
        <xs:annotation>
            <xs:documentation>Date and time of actual gate closure for a
