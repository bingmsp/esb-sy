        </xs:annotation>
        <xs:restriction base="AbstractTypeStatusReason"/>
    </xs:simpleType>
    <xs:simpleType name="GateStatusStartDateTime">
        <xs:annotation>
            <xs:documentation>The general resource status start period.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:complexType name="GeneralResourceAllocationIdentification">
