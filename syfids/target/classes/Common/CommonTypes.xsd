        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="GateArea">
        <xs:annotation>
            <xs:documentation>Identification of Gate area.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GatePodium">
        <xs:annotation>
            <xs:documentation>This field is a free text field to identify a Gate podium. A Gate Podium is where the desk at a gate is not tied to a single individual gate but can be shared between different gates.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GateDescription">
        <xs:annotation>
            <xs:documentation>Description of the gate.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GateLastCallDateTime">
        <xs:annotation>
            <xs:documentation>Time and date when the gate's last call status was
