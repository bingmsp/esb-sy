<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://common.schema.ultra-as.com" targetNamespace="http://common.schema.ultra-as.com" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0">
    <xs:annotation>
        <xs:documentation>XML types used in messages for all services.
        </xs:documentation>
    </xs:annotation>
    <xs:simpleType name="GeneralResourceActualEndDateTime">
        <xs:annotation>
            <xs:documentation>The general resource actual end period.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="GeneralResourceActualStartDateTime">
        <xs:annotation>
            <xs:documentation>The general resource actual start period.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="GeneralResourcePlanEndDateTime">
        <xs:annotation>
            <xs:documentation>The general resource plan end period.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="GeneralResourcePlanStartDateTime">
        <xs:annotation>
            <xs:documentation>The general resource plan start period.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="AbstractTypeStatusReason">
        <xs:annotation>
            <xs:documentation>An Abstract Super Type that defines the general
                attributes of a StatusReason element.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="AccountCode">
        <xs:annotation>
            <xs:documentation>Flight account code; derived from the aircraft's
                registration for GA flights, or the airline IATA code for scheduled
                flights; cash payers will have a code of 000.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ActualFinalApproachTime">
        <xs:annotation>
            <xs:documentation>Actual Date and time of the aircraft's final approach to the airport.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="ActualInBlockTime">
        <xs:annotation>
            <xs:documentation>Actual date and time of arrival on stand for an inbound aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="AirportLocationID">
        <xs:annotation>
            <xs:documentation> AirportLocationID â€“ this should be the IATA airport code for any specific data or null</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
            <xs:minLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ActualInBlockTimes">
        <xs:annotation>
            <xs:documentation>These fields all relate to the time the aircraft arrives on blocks. The elements here include a 'best' value, ActualInBlockTime, as well as values from various sources.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ActualInBlockTime" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualInBlockTimeATCTower" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualInBlockTimeRadar" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualInBlockTimeDGS" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualInBlockTimeSitaMsg" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualInBlockTimeManual" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualInBlockTimeGroundHandler" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ActualLandingTime">
        <xs:annotation>
            <xs:documentation>The time aircraft lands actually on runway</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:complexType name="ActualLandingTimes">
        <xs:annotation>
            <xs:documentation>These fields all relate to the time aircraft lands on runway. The elements here include a 'best' value, ActualLandingTime, as well as values from various sources.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ActualLandingTime" type="ActualLandingTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualLandingTimeManual" type="ActualLandingTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualLandingTimeRadar" type="ActualLandingTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualLandingTimeSitaMsg" type="ActualLandingTime" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ActualOffBlockTime">
        <xs:annotation>
            <xs:documentation>Actual date and time of departure from stand for an outbound aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:complexType name="ActualOffBlockTimes">
        <xs:annotation>
            <xs:documentation>These fields all relate to the time the aircraft pushes back and vacates the parking position. The elements here include a 'best' value, ActualOffBlocksTime, as well as values from various sources.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ActualOffBlockTime" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualOffBlockTimeRadar" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualOffBlockTimeATCTower" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualOffBlockTimeDGS" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualOffBlockTimeSitaMsg" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualOffBlockTimeManual" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualOffBlockTimeGroundHandler" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ActualTakeOffTime">
        <xs:annotation>
            <xs:documentation>Actual time that an aircraft takes off from the runway.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:complexType name="ActualTakeOffTimes">
        <xs:annotation>
            <xs:documentation>These fields all relate to the time that an aircraft takes off from the runway. The elements here include a 'best' value, ActualTakeOffTime, as well as values from various sources.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="ActualTakeOffTime" type="ActualTakeOffTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualTakeOffTimeManual" type="ActualTakeOffTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualTakeOffTimeRadar" type="ActualTakeOffTime" nillable="true" minOccurs="0"/>
            <xs:element name="ActualTakeOffTimeSitaMsg" type="ActualTakeOffTime" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="AdultPassengerCount">
        <xs:annotation>
            <xs:documentation>Total number of adult passengers on board.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="Aircraft">
        <xs:annotation>
            <xs:documentation>All data related to the aircraft.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element name="AircraftCallsign" type="AircraftCallsign" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftOwnerIATACode" type="AircraftOwnerIATACode" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftPassengerCapacity" type="AircraftPassengerCapacity" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftRegistration" type="AircraftRegistration" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftSubtypeIATACode" type="AircraftSubtypeIATACode" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftTypeICAOCode" type="AircraftTypeICAOCode" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftOperator" type="AircraftOperator" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftLoadType" type="AircraftLoadType" nillable="true" minOccurs="0"/>
            <xs:element name="MarshallerRequired" type="MarshallerRequired" nillable="true" minOccurs="0"/>
            <xs:element name="MaintenanceType" type="MaintenanceType" nillable="true" minOccurs="0"/>
            <xs:element name="NoOfTakeoffLandings" type="NoOfTakeoffLandings" nillable="true" minOccurs="0"/>
            <xs:element name="RadioFrequency" type="RadioFrequency" nillable="true" minOccurs="0"/>
            <xs:element name="FleetNumber" type="FleetNumber" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftTailNumber" type="AircraftTailNumber" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftVisitIdentifier" type="AircraftVisitSequence" nillable="true" minOccurs="0"/>
            <xs:element name="SpareAircraftCharacter1" type="SpareAircraftCharacter" nillable="true" minOccurs="0"/>
            <xs:element name="SpareAircraftCharacter2" type="SpareAircraftCharacter" nillable="true" minOccurs="0"/>
            <xs:element name="SpareAircraftDate1" type="SpareAircraftDate" nillable="true" minOccurs="0"/>
            <xs:element name="SpareAircraftDate2" type="SpareAircraftDate" nillable="true" minOccurs="0"/>
            <xs:element name="SpareAircrafttNumber1" type="SpareAircrafttNumber" nillable="true" minOccurs="0"/>
            <xs:element name="SpareAircrafttNumber2" type="SpareAircrafttNumber" nillable="true" minOccurs="0"/>
            <xs:element name="FuelAmount" type="FuelAmount" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="AircraftCallsign">
        <xs:annotation>
            <xs:documentation>Callsign associated with the aircraft.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftCategoryIATACode">
        <xs:annotation>
            <xs:documentation>IATA aircraft type category, this is defined as an NA where the numeric is the number of engines and the alpha is the IATA engine type  (e.g. 3J = 3 jet
                engines, 4T = 4 turboprops, 2P = 2 propellers).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftHeight">
        <xs:annotation>
            <xs:documentation>Height of the aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="999"/>
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftLength">
        <xs:annotation>
            <xs:documentation>Length of the aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="999"/>
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftMovementDateTime">
        <xs:annotation>
            <xs:documentation>Actual or planned time an aircraft arrived/departed
                at/from the stand.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:dateTime"/>
    </xs:simpleType>
    <xs:simpleType name="AircraftMovementSequence">
        <xs:annotation>
            <xs:documentation>The tow number associated with an ordered tow
                sequence. Note: a long data type is used to ensure that XML
                generation tools cater for very large integer values.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:long">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999999999999999999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftMovementTypeCode">
        <xs:annotation>
            <xs:documentation>Code representing the status of an on/off blocks
                aircraft movement.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Archived">
                <xs:annotation>
                    <xs:documentation>Archived but incomplete tow.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Created">
                <xs:annotation>
                    <xs:documentation>Created on system initialisation to represent
                        existing state when cutting over.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Open Tow">
                <xs:annotation>
                    <xs:documentation>Open tow. Allows tow off/on time to be set
                        without having a complete parking sequence.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Restricted Tow">
                <xs:annotation>
                    <xs:documentation>Restricted tow.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Slide Back">
                <xs:annotation>
                    <xs:documentation>Slide back.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Tow">
                <xs:annotation>
                    <xs:documentation>Tow.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftMovementDirection">
        <xs:annotation>
            <xs:documentation>Tow is associated with the arrival or departure.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="FlightDirection"/>
    </xs:simpleType>
    <xs:simpleType name="AircraftNoiseLevel">
        <xs:annotation>
            <xs:documentation>Effective Perceived Noise Level (EPNL) of the
                aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftNoseNumber">
        <xs:annotation>
            <xs:documentation>Nose number of the aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftOwnerIATACode">
        <xs:annotation>
            <xs:documentation>IATA code of the aircraft owning airline.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftPassengerCapacity">
        <xs:annotation>
            <xs:documentation>Maximum number of passengers that an aircraft can
                accommodate.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="999"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftRegistration">
        <xs:annotation>
            <xs:documentation>Aircraft registration.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftRegistrationDescription">
        <xs:annotation>
            <xs:documentation>Description of the aircraft registration.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftSubtypeDescription">
        <xs:annotation>
            <xs:documentation>Description of the aircraft subtype.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="90"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftSubtypeIATACode">
        <xs:annotation>
            <xs:documentation>IATA assigned code for an aircraft subtype.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftTailNumber">
        <xs:annotation>
            <xs:documentation>Tail number of the aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftTerminalCode">
        <xs:annotation>
            <xs:documentation>Terminal code for an aircraft, which may differ
                from the terminal presented to passengers.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="TerminalCode"/>
    </xs:simpleType>
    <xs:simpleType name="AirlineTerminalCode">
        <xs:annotation>
            <xs:documentation>Terminal code associated with the flight's airline
                (the usual location associated with airline resources, which may
                differ from passenger or aircraft terminals).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="TerminalCode"/>
    </xs:simpleType>
    <xs:complexType name="AircraftMovementData">
        <xs:sequence>
            <xs:element name="Identification" type="IdentificationType" nillable="false" minOccurs="0"/>
            <xs:element name="ArrivalIdentification" type="ArrivalIdentificationType" nillable="true" minOccurs="0"/>
            <xs:element name="DepartureIdentification" type="DepartureIdentificationType" nillable="true" minOccurs="0"/>
            <xs:element name="ActualOffBlockTime" type="ActualOffBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="TowOffStand" type="TowOffStand" nillable="true" minOccurs="0"/>
            <xs:element name="TowOffConfirmation" type="TowOffConfirmation" nillable="true" minOccurs="0"/>
            <xs:element name="ActualInBlockTime" type="ActualInBlockTime" nillable="true" minOccurs="0"/>
            <xs:element name="TowOnStand" type="TowOnStand" nillable="true" minOccurs="0"/>
            <xs:element name="TowOnConfirmation" type="TowOnConfirmation" nillable="true" minOccurs="0"/>
            <xs:element name="AircraftRegistration" type="AircraftRegistration" nillable="true" minOccurs="0"/>
            <xs:element name="StandPosition" type="StandPosition" nillable="false" minOccurs="0"/>
            <xs:element name="AircraftMovementTypeCode" type="AircraftMovementTypeCode" nillable="true" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="TowOffStand">
        <xs:annotation>
            <xs:documentation>Stand associated with the tow off.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TowOnStand">
        <xs:annotation>
            <xs:documentation>Stand associated with the tow on.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TowOnConfirmation">
        <xs:annotation>
            <xs:documentation>Status of the on tow (UCT) :U=tow automatically created and not confirmed by user. C=tow automatically created and confirmed by user.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TowOffConfirmation">
        <xs:annotation>
            <xs:documentation>Status of the off tow (UCT) :U=tow automatically created and not confirmed by user. C=tow automatically created and confirmed by user.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftTypeIATACode">
        <xs:annotation>
            <xs:documentation>IATA assigned code for an aircraft type.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftTypeICAOCode">
        <xs:annotation>
            <xs:documentation>ICAO assigned code for an aircraft type.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="4"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftWeight">
        <xs:annotation>
            <xs:documentation>Weight of the aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="99999999"/>
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftWidth">
        <xs:annotation>
            <xs:documentation>Width of the aircraft.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer">
            <xs:maxInclusive value="999"/>
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AirlineDescription">
        <xs:annotation>
            <xs:documentation>Description of the airline.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AirlineIATACode">
        <xs:annotation>
            <xs:documentation>IATA assigned code defining an airline typically
                two character passenger preferred).</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="2"/>
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AirlineICAOCode">
        <xs:annotation>
            <xs:documentation>ICAO assigned code which uniquely defines an
                airline.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:length value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AirlineName">
        <xs:annotation>
            <xs:documentation>Expanded name of the airline.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="30"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftOperator">
        <xs:annotation>
            <xs:documentation>Airline that is operating the aircraft/flight.
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="3"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AircraftLoadType">
        <xs:annotation>
            <xs:documentation>Aircraft load type. This is an airport specific code that maps to a reference dataset
         </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="2"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="Airport">
        <xs:annotation>
            <xs:documentation>All airport related data</xs:documentation>
        </xs:annotation>
        <xs:sequence minOccurs="0">
            <xs:element name="AircraftTerminalCode" type="AircraftTerminalCode" nillable="true" minOccurs="0"/>
            <xs:element name="AirlineTerminalCode" type="AirlineTerminalCode" nillable="true" minOccurs="0"/>
            <xs:element name="BaggageMakeupPosition" type="BaggageMakeupPosition" nillable="true" minOccurs="0" maxOccurs="2"/>
            <xs:element name="MakeupPlannerComment" type="Comment" nillable="true" minOccurs="0"/>
            <xs:element name="BaggageReclaimCarousel" type="BaggageReclaimCarousel" nillable="true" minOccurs="0" maxOccurs="2"/>
            <xs:element name="ReclaimPlannerComment" type="Comment" nillable="true" minOccurs="0"/>
            <xs:element name="BusIsRequired" type="BusIsRequired" nillable="true" minOccurs="0"/>
            <xs:element name="CheckinRangeIndicator" type="CheckinRangeIndicator" nillable="true" minOccurs="0"/>
            <xs:element name="Checkin" type="Checkin" minOccurs="0" maxOccurs="2"/>
            <xs:element name="FlightTerminalCode" type="FlightTerminalCode" nillable="true" minOccurs="0"/>
            <xs:element name="Gate" type="Gate" nillable="true" minOccurs="0" maxOccurs="2"/>
            <xs:element name="RampText" type="RampText" nillable="true" minOccurs="0"/>
            <xs:element name="PreviousGateIdentity" type="GateNumber" nillable="true" minOccurs="0"/>
            <xs:element name="GatePlannerComment" type="Comment" nillable="true" minOccurs="0"/>
