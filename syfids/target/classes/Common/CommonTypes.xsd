            <xs:enumeration value="Closed"/>
            <xs:enumeration value="Final Call"/>
            <xs:enumeration value="Last Call"/>
            <xs:enumeration value="Servicable"/>
            <xs:enumeration value="Unservicable"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GateStatusEndDateTime">
        <xs:annotation>
