<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.iata.org/IATA/2007/00" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.iata.org/IATA/2007/00" elementFormDefault="qualified" version="2.000" id="IATA2012.1">
	<xs:include schemaLocation="IATA_SimpleTypes.xsd"/>
	<xs:include schemaLocation="IATA_CommonTypes.xsd"/>
	<xs:simpleType name="DepartureArrivalType">
		<xs:annotation>
			<xs:documentation>Used to specify if information is for a departure or an arrival.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="Departure"/>
			<xs:enumeration value="Arrival"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DestinationType">
		<xs:annotation>
			<xs:documentation>Used to specify the destination type such as local, transit, or transfer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="Local"/>
			<xs:enumeration value="Transfer"/>
			<xs:enumeration value="Transit"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="FuelQualifierEnumType">
		<xs:annotation>
			<xs:documentation>Fuel qualifier to specify the fuel being measured. An "Other_" value to support open enumeration list.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="FuelUplift">
				<xs:annotation>
					<xs:documentation xml:lang="en">The amount of fuel the fuelling company should load on the aircraft.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FuelOnboard">
				<xs:annotation>
					<xs:documentation xml:lang="en">The amount of fuel the aircraft has in its tanks while on the ramp/stand.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TripFuel">
				<xs:annotation>
					<xs:documentation xml:lang="en">The amount of fuel the flight planning system predicts the aircraft will burn in flight.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TakeoffFuel">
				<xs:annotation>
					<xs:documentation xml:lang="en">The amount of fuel the aircraft has in its tanks at takeoff (fuel  onboard less the amount burned to get to start of runway).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other_">
				<xs:annotation>
					<xs:documentation xml:lang="en"> "Other_" as enumerated value for  open enumeration support. See AccommodationClass. </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MeasurementUnitType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Kilogram"/>
			<xs:enumeration value="Pound"/>
			<xs:enumeration value="Ton"/>
			<xs:enumeration value="Tonne"/>
			<xs:enumeration value="Litre"/>
			<xs:enumeration value="USGallon"/>
			<xs:enumeration value="ImperialGallon"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="BaggageLoadType">
		<xs:annotation>
			<xs:documentation>Summary information of baggage weight and count loaded on aircraft,   optionally summarized by destination type, and service class</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="BagCount" minOccurs="0" maxOccurs="50">
				<xs:annotation>
					<xs:documentation>Number of bags for a destination type and cabin class in a specified location.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:positiveInteger">
							<xs:attribute name="Location" type="xs:string" use="required">
								<xs:annotation>
									<xs:documentation>The location of the baggage count on the aircraft., i.e. Bin name or ULD ID. Repeating group to cover the different bins and total.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Weight" type="MeasurementType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Weight of baggage loaded on the aircraft </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ServiceClass" type="IATA_CodeType">
			<xs:annotation>
				<xs:documentation>Specifies a class of service for the baggage loaded information.  Refer to IATA PADIS Code List for data element 9873.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DestinationType" type="DestinationType">
			<xs:annotation>
				<xs:documentation>Used to specify the destination type such as local, transit or transfer.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RepeatIndex">
			<xs:annotation>
				<xs:documentation>Identifies an order for a repeating item.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DeadLoadType">
		<xs:sequence>
			<xs:element name="Type" type="IATA_CodeType">
				<xs:annotation>
					<xs:documentation>Type of dead load. Refer to IATA PADIS code list for data element 7085</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Weight" type="MeasurementType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A weight and unit of measurement.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="DestinationType" type="DestinationType"/>
	</xs:complexType>
	<xs:complexType name="FlightLegIdentifierType">
		<xs:annotation>
			<xs:documentation>Identifies a flight leg.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Airline">
				<xs:annotation>
					<xs:documentation>The operating airline code.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="AirlineType">
							<xs:attribute name="CodeContext" type="IATA_CodeType" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the context of the airline code, such as IATA or ICAO.  Refer to IATA PADIS Code List for data element 3055.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="FlightNumber" type="NumericStringLength1to4">
				<xs:annotation>
					<xs:documentation>The actual flight number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="OperationalSuffix" type="OperationalSuffixType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The operational suffix for the flight.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DepartureAirport">
				<xs:annotation>
					<xs:documentation>The scheduled departure airport.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="StationType">
							<xs:attribute name="CodeContext" type="IATA_CodeType" use="optional">
								<xs:annotation>
									<xs:documentation>Specifies the context of the airport code, such as IATA or ICAO.  Refer to IATA PADIS Code List for data element 3055.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ArrivalAirport">
				<xs:annotation>
					<xs:documentation>The scheduled arrival airport.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="StationType">
							<xs:attribute name="CodeContext" type="IATA_CodeType" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">Specifies the context of the airport code, such as IATA or ICAO.  Refer to IATA PADIS Code List for data element 3055.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="OriginDate" type="xs:date">
				<xs:annotation>
					<xs:documentation>The scheduled flight origin date based on the flight not the flight leg.  This date must not change once intialized in an AIDX message.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RepeatNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The repeat or departure attempt number.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="Numeric1to99">
							<xs:attribute name="CurrentInd" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>When true the leg with this repeat number operated the flight and when  false this leg has been replaced by another leg.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FlightLegType">
		<xs:annotation>
			<xs:documentation> Used to specify all the information for a flight leg.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="LegIdentifier" type="FlightLegIdentifierType">
				<xs:annotation>
					<xs:documentation>Uniquely identify a flight leg.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialAction" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A special action which should be taken on the record.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="Delete"/>
						<xs:enumeration value="LockDown"/>
						<xs:enumeration value="DoNotDisplay"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LegData">
				<xs:annotation>
					<xs:documentation>Flight leg information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="OperationalStatus" nillable="true" minOccurs="0" maxOccurs="5">
							<xs:annotation>
								<xs:documentation>Used to inform the airline and airport operational staff concerning the status of a flight leg.  Refer to IATA PADIS Code List for data element 1245 and 2005</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="IATA_CodeType">
										<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
											<xs:annotation>
												<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="CodeContext" type="xs:string">
											<xs:annotation>
												<xs:documentation>Used to specify if the operational status code is from IATA PADIS Code List for data element 1245 or 2005.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="PublicStatus" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>A list of flight leg statuses that can be displayed to the public.  Refer to IATA PADIS Code List for data element 1245 and 2005.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="IATA_CodeType">
										<xs:attribute name="CodeContext" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation xml:lang="en">Used to specify if the remark text code is from IATA PADIS Code List for data element 1245 or 2005.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="OperatingAlliance" type="IATA_CodeType" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The operating alliance for the flight.  Refer to IATA PADIS Code List for data element 9906.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ServiceType" type="ServiceType" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The IATA flight service type of the operating flight.  Use SSIM service types.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="TechnicalStopInd" type="xs:boolean" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>If true, this is a technical stop.  If false, it is not a technical stop.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="DepSecurityCheckInd" type="xs:boolean" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>If true, additional security checks are required for departure.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="ArrSecurityCheckInd" type="xs:boolean" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>If true, addtional security checks are required for the arrival part of the flight leg.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="EstFlightDuration" type="xs:duration" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>The estimated flight duration time (the time  from off blocks to block time).</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="OwnerAirline" nillable="true" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Aircraft owner information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Airline">
										<xs:annotation>
											<xs:documentation>The aircraft owner code.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="AirlineType">
													<xs:attribute name="CodeContext" type="IATA_CodeType" use="optional">
														<xs:annotation>
															<xs:documentation xml:lang="en">Specifies the context of the airline code, such as IATA or ICAO.  Refer to IATA PADIS Code List for data element 3055.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="FlightNumber" type="NumericStringLength1to4" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The aircraft owner flight number.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="OperationalSuffix" type="OperationalSuffixType" nillable="true" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The operational suffix for the flight.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="PlannedArrivalAptHistory" nillable="true" minOccurs="0" maxOccurs="10">
							<xs:annotation>
								<xs:documentation>An airport where the leg has either arrived or is planned to arrive.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="StationType">
										<xs:attribute name="CodeContext" type="IATA_CodeType" use="optional">
											<xs:annotation>
												<xs:documentation xml:lang="en">Specifies the context of the airport code, such as IATA or ICAO.  Refer to IATA PADIS Code List for data element 3055.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
											<xs:annotation>
												<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="SpecialEmphasis" nillable="true" minOccurs="0" maxOccurs="3">
							<xs:annotation>
								<xs:documentation>Used to specify if the flight requires special attention or handling.  Refer to IATA PADIS Code List EMP.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="IATA_CodeType">
										<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
											<xs:annotation>
												<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="IrregularityDelay" nillable="true" minOccurs="0" maxOccurs="8">
							<xs:annotation>
								<xs:documentation>The reason and duration for a departure or arrival irregularity delay.  </xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ReasonCode" type="AlphaNumericStringLength1to3">
										<xs:annotation>
											<xs:documentation>Used to specify the reason for a departure irregularity delay. </xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="Duration" type="xs:duration" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The duration of the delay.  Format PT2H15M.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="DepartureOrArrival" type="DepartureArrivalType" use="required">
									<xs:annotation>
										<xs:documentation xml:lang="en">Specifies if the delay is for a departure or an arrival.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
									<xs:annotation>
										<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="CabinClass" minOccurs="0" maxOccurs="5">
							<xs:annotation>
								<xs:documentation>A cabin class for this flight leg.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="InflightService" nillable="true" minOccurs="0" maxOccurs="20">
										<xs:annotation>
											<xs:documentation>The facilities/services off ered during this flight leg.  Refer to IATA PADIS Code List 9932.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="IATA_CodeType">
													<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
														<xs:annotation>
															<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="InflightMealService" nillable="true" minOccurs="0" maxOccurs="20">
										<xs:annotation>
											<xs:documentation>The meal services offered during this flight.  Refer to IATA PADIS Code List 7161.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="IATA_CodeType">
													<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
														<xs:annotation>
															<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="PaxCount" nillable="true" minOccurs="0" maxOccurs="3">
										<xs:annotation>
											<xs:documentation>The passenger type and count.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="Numeric0to999">
													<xs:attribute name="Qualifier" type="IATA_CodeType" use="required">
														<xs:annotation>
															<xs:documentation xml:lang="en">The type of passenger count.  Refer to IATA PADIS Code List for data element 6353.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="Usage" type="UsageType" use="required">
														<xs:annotation>
															<xs:documentation>Used to indicate if this is a planned or actual count.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
														<xs:annotation>
															<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="DestinationType" type="DestinationType">
														<xs:annotation>
															<xs:documentation>Used to specify the destination type such as transit or local or transfer for this pasenger count.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="SeatCapacity" nillable="true" minOccurs="0">
										<xs:annotation>
											<xs:documentation>The seating capacity  by cabin class.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="Numeric0to999"/>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="Class" type="IATA_CodeType" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">Specifies a class of service for which the inflight service or meal applies.  Refer to IATA PADIS Code List for data element 9873.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
									<xs:annotation>
										<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="SpecialCargo" nillable="true" minOccurs="0" maxOccurs="20">
							<xs:annotation>
								<xs:documentation>Identifies any special cargo on board.  Refer to IATA PADIS Code List CAR.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="IATA_CodeType">
										<xs:attribute name="RepeatIndex" type="Numeric1to99" use="optional">
											<xs:annotation>
												<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="CodeShareInfo" nillable="true" minOccurs="0" maxOccurs="20">
							<xs:annotation>
								<xs:documentation>The code share information for this flight leg.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Airline">
										<xs:annotation>
											<xs:documentation>The marketing airline code.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="AirlineType">
													<xs:attribute name="CodeContext" type="IATA_CodeType" use="optional">
														<xs:annotation>
															<xs:documentation xml:lang="en">Specifies the context of the airline code, such as IATA or ICAO.  Refer to IATA PADIS Code List for data element 3055.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
