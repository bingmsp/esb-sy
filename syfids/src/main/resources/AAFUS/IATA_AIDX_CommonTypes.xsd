																		<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
																	</xs:annotation>
																</xs:attribute>
															</xs:extension>
														</xs:simpleContent>
													</xs:complexType>
												</xs:element>
												<xs:element name="RemoteOperationalGate" nillable="true" minOccurs="0" maxOccurs="3">
