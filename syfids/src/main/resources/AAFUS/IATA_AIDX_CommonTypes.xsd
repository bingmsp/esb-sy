												<xs:documentation>Identifies an order for a repdeating item.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:sequence>
							<xs:element name="AssociatedFlightLegSchedule" nillable="true" minOccurs="0" maxOccurs="20">
