									</xs:element>
									<xs:element name="Fuel" type="FuelDataType" minOccurs="0" maxOccurs="9">
										<xs:annotation>
											<xs:documentation>The measurement of fuel for a specified state of the aircraft movment.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="ClearanceAgreement" type="IATA_CodeType" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Specifies the type of clearance agreement.  Refer to IATA PADIS Code List for data element 9970.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="PublicFlightDisplay" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Specifies how a flight should be shown on a public display.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="AirlineType">
										<xs:annotation>
											<xs:documentation>Specifies how the carrier code should be shown on a public display.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:simpleContent>
												<xs:extension base="AirlineType">
													<xs:attribute name="CodeContext" type="IATA_CodeType">
														<xs:annotation>
															<xs:documentation>Specifies the context of the airline code, such as IATA or ICAO.  Refer to IATA PADIS Code List for data element 3055.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:simpleContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="FlightNumber" type="NumericStringLength1to4">
										<xs:annotation>
											<xs:documentation>The flight number to be shown in a public display.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="FlightClassification" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The commercial name for express or other sub-carriers for the operating flight.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="0"/>
								<xs:maxLength value="10"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="InternationalStatus">
						<xs:annotation>
							<xs:documentation>Specifies if the flight is international or domestic.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:NMTOKEN">
								<xs:enumeration value="International">
									<xs:annotation>
										<xs:documentation xml:lang="en">Used to specify if the flight leg is international.</xs:documentation>
									</xs:annotation>
								</xs:enumeration>
								<xs:enumeration value="Domestic">
									<xs:annotation>
										<xs:documentation xml:lang="en">Used to specify if the flight leg is domestic.</xs:documentation>
									</xs:annotation>
								</xs:enumeration>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="TPA_Extension" type="TPA_ExtensionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Allows extensions to be added to the IATA specification per trading partner agreement.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FuelDataType">
		<xs:annotation>
			<xs:documentation>The measurement of fuel for a specified state of the aircraft movment.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="FuelQualifierType">
				<xs:annotation>
					<xs:documentation>Type of  fuel data: Fuel Uplift, Fuel Onboard, Trip Fuel, Take off Fuel </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Quantity" type="MeasurementType">
				<xs:annotation>
					<xs:documentation>The quanity of fuel and the unit of measurement.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FuelQualifierType">
		<xs:annotation>
			<xs:documentation>Fuel qualifier with an associated attribute to capture additional enumerated vales as agreed upon beteen trading partners.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="FuelQualifierEnumType">
				<xs:attribute name="extension" type="xs:string" use="optional">
					<xs:annotation>
						<xs:documentation>A string value that represents an "Other_", or extended, enumerated value agreed upon between trading partners.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="MeasurementType">
		<xs:annotation>
			<xs:documentation>A quantitative measured value in Unit of measurement</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="MeasurementUnit" type="MeasurementUnitType">
					<xs:annotation>
						<xs:documentation>The unit used to measure the weight.
						</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="TPA_ExtensionsType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Allows extensions to be added to the IATA specification per trading partner agreement.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
