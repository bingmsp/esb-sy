    <xs:simpleType name="ItemDescription">
        <xs:annotation>
            <xs:documentation>Item Description</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="240"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ItemType">
        <xs:annotation>
            <xs:documentation>Item Description</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="60"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="JournalName">
        <xs:annotation>
            <xs:documentation>Journal name</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="80"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="JournalDescription">
        <xs:annotation>
            <xs:documentation>Journal Description</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="180"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="JournalLineDescription">
        <xs:annotation>
            <xs:documentation>Journal Line Description</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="180"/>
        </xs:restriction>
    </xs:simpleType>
