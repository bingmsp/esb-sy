        <xs:annotation>
            <xs:documentation>Journal Entry Category Name</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="JESourceName">
        <xs:annotation>
            <xs:documentation>Journal Entry Source Name</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="LedgerID">
        <xs:annotation>
            <xs:documentation>Identification of the Ledger</xs:documentation>
        </xs:annotation>
