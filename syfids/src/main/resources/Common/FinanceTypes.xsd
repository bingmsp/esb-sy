            <xs:element name="CustomerBilling" type="CustomerBilling"/>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="FreightTerms">
        <xs:annotation>
            <xs:documentation>Describes what is being shipped</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GeneralLedgerAccount">
        <xs:annotation>
            <xs:documentation>General ledger account</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="22"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="GLCodeCombination">
        <xs:annotation>
            <xs:documentation>General ledger account code</xs:documentation>
