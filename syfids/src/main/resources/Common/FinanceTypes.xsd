        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ItemCode">
        <xs:annotation>
            <xs:documentation>Item code</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
