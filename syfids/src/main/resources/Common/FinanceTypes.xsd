<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://finance.common.schema.ultra-as.com" xmlns:backoffice="http://backoffice.common.schema.ultra-as.com" targetNamespace="http://finance.common.schema.ultra-as.com" elementFormDefault="qualified" attributeFormDefault="unqualified" version="2.0">
    <xs:annotation>
        <xs:documentation>XML types used in messages for finance information</xs:documentation>
    </xs:annotation>
    <xs:import namespace="http://backoffice.common.schema.ultra-as.com" schemaLocation="../Common/BackOfficeDataTypes.xsd"/>
    <xs:complexType name="AccountingPeriodStatus">
        <xs:annotation>
            <xs:documentation>Information regarding an available exchange rate</xs:documentation>
