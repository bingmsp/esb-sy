    </xs:simpleType>
    <xs:simpleType name="CalenderMonth">
        <xs:annotation>
            <xs:documentation>Month</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="January"/>
            <xs:enumeration value="February"/>
            <xs:enumeration value="March"/>
            <xs:enumeration value="April"/>
            <xs:enumeration value="May"/>
            <xs:enumeration value="June"/>
            <xs:enumeration value="July"/>
            <xs:enumeration value="August"/>
            <xs:enumeration value="September"/>
            <xs:enumeration value="October"/>
            <xs:enumeration value="November"/>
            <xs:enumeration value="December"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CategoryID">
        <xs:annotation>
            <xs:documentation>category unique identifier</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="40"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ChargeAccountSegment">
        <xs:annotation>
            <xs:documentation>Chart of Account Segment</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ChartOfAccounts">
        <xs:annotation>
            <xs:documentation>Information regarding the Chart of Account</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="LedgerID" type="LedgerID" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="Value" type="Value" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="Description" type="Description" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="OrganizationID" type="OrganizationID" nillable="true" minOccurs="0" maxOccurs="1"/>
            <xs:element name="GLCodeCombination" type="GLCodeCombination" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="GLOrder" type="GLOrder" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="Segment" type="Segment" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="EnabledFlag" type="xs:boolean" nillable="false" minOccurs="0" maxOccurs="1"/>
            <xs:element name="StartDate" type="xs:date" nillable="true" minOccurs="0"/>
            <xs:element name="EndDate" type="xs:date" nillable="true" minOccurs="0"/>
            <xs:element name="GLEnabledFlag" type="xs:boolean" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="GLStartDate" type="xs:date" nillable="true" minOccurs="0" maxOccurs="1"/>
            <xs:element name="GLEndDate" type="xs:date" nillable="true" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="City">
        <xs:annotation>
            <xs:documentation>Name of the city</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="Comm">
        <xs:annotation>
            <xs:documentation>Communication information of supplier</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="AreaCode" type="AreaCode" nillable="true" minOccurs="0" maxOccurs="1"/>
            <xs:element name="Number" type="DeviceNumber" nillable="true" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="Comments">
        <xs:annotation>
            <xs:documentation>Generic type for holding comments</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="240"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CommentsExtended">
        <xs:annotation>
            <xs:documentation>Generic type for holding extended comments</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="480"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactDepartment">
        <xs:annotation>
            <xs:documentation>Supplier contact department</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="230"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactFirstName">
        <xs:annotation>
            <xs:documentation>First name of the supplier contact</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactLastName">
        <xs:annotation>
            <xs:documentation>Last name of the supplier contact</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactMiddleName">
        <xs:annotation>
            <xs:documentation>Middle name of the supplier contact</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactID">
        <xs:annotation>
            <xs:documentation>Supplier contact id</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:integer"/>
    </xs:simpleType>
    <xs:simpleType name="ContactSupplierSiteCode">
        <xs:annotation>
            <xs:documentation>Supplier site code</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContactTitle">
        <xs:annotation>
            <xs:documentation>Prefix for the supplier contact (e.g. Mr, Mrs, etc)</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="5"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ConversionRate">
        <xs:annotation>
            <xs:documentation>Conversion Rate of the currency</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ConversionType">
        <xs:annotation>
            <xs:documentation>Corporate or User Conversion Type</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="User"/>
            <xs:enumeration value="Corporate"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Country">
        <xs:annotation>
            <xs:documentation>Country of residence</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="100"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CurrencyCode">
        <xs:annotation>
            <xs:documentation>Currency Unique identifier</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="CustomerBilling">
        <xs:annotation>
            <xs:documentation>Customer Billing Information</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="CustomerID" type="CustomerID" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="CustomerName" type="CustomerName" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="BillingMonth" type="CalenderMonth" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="Amount" type="BillAmount" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ReBill" type="xs:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="CustomerID">
        <xs:annotation>
            <xs:documentation>Customer identifier</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CustomerName">
        <xs:annotation>
            <xs:documentation>Name of the customer</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="Description">
        <xs:annotation>
            <xs:documentation>Description of the ledger</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DestinationTypeCode">
        <xs:annotation>
            <xs:documentation>Requisition Destination type</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DeviceNumber">
        <xs:annotation>
            <xs:documentation>Device communication number</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="EmailAddress">
        <xs:annotation>
            <xs:documentation>Email address</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="200"/>
        </xs:restriction>
    </xs:simpleType>
