        <xs:sequence>
            <xs:element name="LedgerID" type="LedgerID" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="FinancialPeriodName" type="FinancialPeriodName" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="StartDate" type="xs:date" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="EndDate" type="xs:date" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="YearStartDate" type="xs:date" nillable="false" minOccurs="1" maxOccurs="1"/>
