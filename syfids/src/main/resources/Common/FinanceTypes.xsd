        <xs:restriction base="xs:string">
            <xs:maxLength value="12"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ExchangeRate">
        <xs:annotation>
            <xs:documentation>Information regarding an available exchange rate</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="SourceCurrency" type="CurrencyCode" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="TargetCurrency" type="CurrencyCode" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ActiveDate" type="xs:date" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ExpireDate" type="xs:date" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ConversionType" type="ConversionType" nillable="false" minOccurs="1" maxOccurs="1"/>
            <xs:element name="ConversionRate" type="ConversionRate" nillable="false" minOccurs="1" maxOccurs="1"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ExpenditureType">
        <xs:annotation>
            <xs:documentation>Project accounting expenditure type</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="25"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="FinanceData">
        <xs:annotation>
            <xs:documentation>Information regarding an available exchange rate</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:element name="ExchangeRate" type="ExchangeRate" minOccurs="0" maxOccurs="1"/>
            <xs:element name="AccountingPeriodStatus" type="AccountingPeriodStatus" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ChartOfAccounts" type="ChartOfAccounts" minOccurs="0" maxOccurs="1"/>
            <xs:element name="ServicesReceipts" type="ServicesReceipts" minOccurs="0" maxOccurs="1"/>
            <xs:element name="PurchaseOrderStatus" type="PurchaseOrderStatus" minOccurs="0" maxOccurs="1"/>
            <xs:element name="SupplierInformation" type="SupplierInformation" minOccurs="0" maxOccurs="1"/>
        </xs:choice>
    </xs:complexType>
    <xs:simpleType name="FinanceDataCategory">
        <xs:annotation>
            <xs:documentation>The finance data categories</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="All"/>
            <xs:enumeration value="ExchangeRate"/>
            <xs:enumeration value="AccountingPeriodStatus"/>
            <xs:enumeration value="ChartOfAccounts"/>
            <xs:enumeration value="ServicesReceipts"/>
            <xs:enumeration value="PurchaseOrderStatus"/>
            <xs:enumeration value="SupplierInformation"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FinancialPeriodName">
        <xs:annotation>
            <xs:documentation>Name of the financial period</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>
